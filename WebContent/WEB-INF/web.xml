<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>TimeCapsule-WebApp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.timecapsule.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Signup</servlet-name>
    <servlet-class>com.timecapsule.Signup</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Mainpage</servlet-name>
    <servlet-class>com.timecapsule.Main</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Mainpage</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Mainpage2</servlet-name>
    <servlet-class>com.timecapsule.Main2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Mainpage2</servlet-name>
    <url-pattern>/main2</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Mainpage3</servlet-name>
    <servlet-class>com.timecapsule.Main3</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Mainpage3</servlet-name>
    <url-pattern>/main3</url-pattern>
  </servlet-mapping>
</web-app>